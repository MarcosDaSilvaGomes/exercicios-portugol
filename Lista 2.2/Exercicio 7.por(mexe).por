programa
{
	
	funcao inicio()
	{
		inteiro a, b, c, d, f, y, x

		escreva("Digite os coeficientes a, b, c, d, e, f\n")
		leia(a, b, c, d, f)

	        x = ((c * f) - (b * g)) / ((a * f) - (b * d))

		 y = ((a * g) - (c * d)) / ((a * f) - (b * d))

		escreva("Valor x: ", x, "\nValor y: ", y)
	}
}
